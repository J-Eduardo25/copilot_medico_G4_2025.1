{
    "manifest_version" : 3,
    "version": "1.0.0",
    "name": "Copmed Extension",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "storage",
        "*://localhost/*"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "32": "copilot-icon.png",
        "48": "CopMed_LogoBig.png",
        "128": "CopMed_LogoBig.png"
    }
}
